  "scripts": {
    "build": "nest build",
    "esbuild": "node bundler.mjs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "npx release-it",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- migration:run -t false -d ./src/dataSource.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/dataSource.ts",
    "run-fixture": "npx ts-node ./test/runFixture.ts"
  },
  "dependencies": {
